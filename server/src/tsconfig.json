{
  "compilerOptions": {
    "allowJs": true,
    "strict": true,
    "module": "Node16",
    "forceConsistentCasingInFileNames": true,
    "target": "es2024",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "noEmit": true,
    "jsx": "react",
    "declaration": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true
  }
}